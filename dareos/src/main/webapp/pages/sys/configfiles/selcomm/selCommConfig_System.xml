    <table id="10" pyName="(
    	select 
    		a.RYBH,
	       a.XM,
	       c.BMBH,
	       c.BMMC,
	       b.JGBH,
	       b.JGMC,
	       c.BMXXID,
	       b.JGXXID,
	       a.RYXXID,
	       b.ZTXXID,
	       b.IS_DRP_LEDGER
	  from T_SYS_RYXX a
	  left join T_SYS_JGXX b
	    on a.jgxxid = b.jgxxid
	  left join T_SYS_BMXX c
	    on a.bmxxid = c.bmxxid
	 where a.ryzt = '启用'
	   and a.delflag = 0
	   and b.jgzt = '启用'
	   and b.delflag = 0
	   and c.bmzt = '启用'
	   and c.DELFLAG = 0
    	)"	lgName="人员信息" orderName="rybh" orderType="ASC">
